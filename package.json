{
  "name": "rs-cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation NEXT_BUILD_SKIP_DB=true next build",
    "build:production": "cross-env NODE_ENV=production NODE_OPTIONS=--no-deprecation NEXT_BUILD_SKIP_DB=true NEXT_TELEMETRY_DISABLED=1 next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.50.0",
    "@payloadcms/next": "^3.50.0",
    "@payloadcms/payload-cloud": "^3.50.0",
    "@payloadcms/plugin-search": "^3.50.0",
    "@payloadcms/richtext-lexical": "^3.50.0",
    "@payloadcms/storage-s3": "^3.50.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.1",
    "graphql": "^16.8.1",
    "next": "^15.4.5",
    "openai": "^5.12.2",
    "payload": "^3.50.0",
    "payload-plugin-algolia": "^0.1.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^5.4.54",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^24.2.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "form-data": "^4.0.4",
    "knowhub": "^1.1.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
